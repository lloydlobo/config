!https://github.com/k-vernooy/dotfiles/blob/master/home/Xresources
! run $ xrdb .Xresources after change
!
Xft*dpi:          96
Xft*antialias:    true
Xft*hinting:      full


! Set terminal color scheme
! TODO: SET DIFFERENT BRIGHT/DARK VARIANTS

! special
*.foreground: #707173
*.cursorColor: #708183

! black
*.color0: #001e26
*.color8: #465a61

! red
*.color1: #e46368
*.color9: #e46368

! green
*.color2:  #fa7a80
*.color10: #fa7a80
! #57ab33


!f2d472 (light yellow)

! yellow
*.color3: #ffa578
*.color11: #ffa578

! blue
*.color4: #5c94e0
*.color12: #5c94e0

! magenta
*.color5: #d295f6
*.color13: #d295f6

! cyan
*.color6: #95bef8
*.color14: #95bef8

! white
*.color7: #e4e9ff
*.color15: #e4e9ff

! basic configuration
URxvt.scrollBar: false
URxvt.cursorBlink: true
URxvt.internalBorder: 4
URxvt.urlLauncher: firefox

URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

! URxvt.transparent: true
! URxvt.shading:20

! Included this so you know how to make your foreground not match the color of your background
URxvt.foreground: #eeeeee

! font options
! URxvt.font:       xft:Hack Nerd Font Mono: size=10: antialiasing=true
URxvt.font:       xft:Fira Code: size=12.5: antialiasing=true

! Enable normal copy/paste
URxvt.iso14755:        false
URxvt.iso14755_52:     false
URxvt.keysym.C-M-c:    builtin-string:
URxvt.keysym.C-M-v:    builtin-string:

! Bind Ctrl-Shift-c & Ctrl-Shift-v to copy and paste
URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
URxvt.keysym.C-S-0x56: eval:paste_clipboard

urxvt*depth:32

